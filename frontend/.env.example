# Supabase Authentication (Required)
NEXT_PUBLIC_SUPABASE_URL="YOUR_SUPABASE_URL"
NEXT_PUBLIC_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"

# Organization Access Control (Required)
# Comma-separated list of allowed email domains (e.g., "company.com,partner.org")
NEXT_PUBLIC_ALLOWED_EMAIL_DOMAINS="example.com"

# LangGraph Cloud API (Required)
NEXT_PUBLIC_LANGGRAPH_API_URL="YOUR_LANGGRAPH_CLOUD_URL"
LANGGRAPH_API_KEY="YOUR_LANGGRAPH_API_KEY"

# API Configuration (Required)
NEXT_PUBLIC_API_URL="http://localhost:3000/api"  # Used by frontend components
NEXT_PUBLIC_API_BASE_URL="http://localhost:3000/api"  # Alternative name (same value)
API_BASE_URL="YOUR_LANGGRAPH_CLOUD_URL"  # Used by API routes server-side

# LangSmith Configuration (Optional but recommended)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT="https://api.smith.langchain.com"
LANGCHAIN_API_KEY="YOUR_LANGSMITH_KEY"
LANGSMITH_API_KEY="YOUR_LANGSMITH_KEY"  # Same as LANGCHAIN_API_KEY
LANGCHAIN_PROJECT="YOUR_PROJECT_NAME"

# Model Configuration (if not using LangGraph Cloud defaults)
# OPENAI_API_KEY="YOUR_OPENAI_API_KEY"